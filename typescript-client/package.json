{
  "name": "quivr-typescript-client",
  "version": "1.0.0",
  "description": "TypeScript client for Quivr Brain Service",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:legacy": "eslint src --ext .ts -c .eslintrc.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "check": "npm run type-check && npm run lint && npm run format:check"
  },
  "keywords": [
    "quivr",
    "rag",
    "ai",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/form-data": "^2.5.2",
    "@types/multer": "^2.0.0",
    "axios": "^1.6.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "form-data": "^4.0.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.6",
    "eslint": "^9.15.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "prettier": "^3.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.15.0"
  }
}
